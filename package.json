{
  "name": "fulltilt",
  "version": "1.0.0",
  "description": "Full Tilt is a fast and modern Hugo theme",
  "main": "index.js",
  "repository": "https://github.com/sscargal/fulltilt-hugo-theme",
  "author": "Steve Scargall",
  "license": "GNU GPLv3",
  "scripts": {
    "start": "hugo server --disableFastRender",
    "start-examplesite": "hugo server -s exampleSite --themesDir=../.. --disableFastRender",
    "build-examplesite": "NODE_ENV=production hugo -s exampleSite --themesDir=../.. --gc",
    "build": "NODE_ENV=production hugo --gc"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "tailwindcss": "^2.2.19"
  }
}
